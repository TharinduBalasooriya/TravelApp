<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travelapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CardDetails"></activity>
        <activity android:name=".SearchUserID" />
        <activity android:name=".ViewUserDetails" />
        <activity android:name=".userProfile" />
        <activity android:name=".hotel_booking_form2" />
        <activity android:name=".hotelBookinForm1" />
        <activity android:name=".Profile" />
        <activity android:name=".RestaurantsList" />
        <activity android:name=".FoodCategory" />
        <activity android:name=".HotelList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>